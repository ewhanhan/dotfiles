[
    {
        "context": "Dock",
        "bindings": {
            "ctrl-a h": [
                "workspace::ActivatePaneInDirection",
                "Left"
            ],
            "ctrl-a l": [
                "workspace::ActivatePaneInDirection",
                "Right"
            ],
            "ctrl-a k": [
                "workspace::ActivatePaneInDirection",
                "Up"
            ],
            "ctrl-a j": [
                "workspace::ActivatePaneInDirection",
                "Down"
            ]
        }
    },
    {
        "context": "Workspace",
        "bindings": {
            "space f f": "file_finder::Toggle",
            "space f e": "workspace::ToggleLeftDock",
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "j k": [
                "workspace::SendKeystrokes",
                "escape"
            ]
        }
    },
    {
        "context": "VimControl && !menu",
        "bindings": {
            "K": "pane::ActivateNextItem",
            "J": "pane::ActivatePrevItem"
        }
    }
]
